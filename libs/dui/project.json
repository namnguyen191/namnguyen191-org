{
  "name": "dui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dui/src",
  "prefix": "namnguyen191",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/dui/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/dui/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/dui/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/dui/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p tsconfig.lib.json --noEmit"],
        "cwd": "libs/dui",
        "forwardAllArgs": false
      }
    }
  }
}
