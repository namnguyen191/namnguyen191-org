{
  "name": "graphql-tutorial-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/graphql-tutorial-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/graphql-tutorial-server",
        "main": "apps/graphql-tutorial-server/src/main.ts",
        "tsConfig": "apps/graphql-tutorial-server/tsconfig.app.json",
        "assets": ["apps/graphql-tutorial-server/src/assets"],
        "webpackConfig": "apps/graphql-tutorial-server/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "graphql-tutorial-server:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "graphql-tutorial-server:build:development"
        },
        "production": {
          "buildTarget": "graphql-tutorial-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p tsconfig.app.json --noEmit"],
        "cwd": "apps/graphql-tutorial-server",
        "forwardAllArgs": false
      }
    }
  },
  "tags": []
}
